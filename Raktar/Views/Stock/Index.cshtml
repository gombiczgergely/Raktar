@model IEnumerable<Raktar.Models.Stock>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Equipment.EqiupmentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Equipment.Describe)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InStock)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SQuantity)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Equipment.EqiupmentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Equipment.Describe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InStock)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SQuantity)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.StockID }) |
            @Html.ActionLink("Details", "Details", new { id = item.StockID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.StockID })
        </td>
    </tr>
}

</table>
